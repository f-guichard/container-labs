###########      System Information      ############
set / system information location site-travee-rack-pos
set / system name host-name PP5GS2ND1MUTLF01
set / system name domain-name pprod.3gpp.mobi.orange.net
set / system dns network-instance mgmt
set / system dns server-list [1.1.1.1, 8.8.8.8, 8.8.4.4]
set / system dns host-entry wallix-bastion { ipv4-address 10.140.7.7 ipv6-address 2001:db8:f411:d1::/48  }
##########       OOB Management          ############
set / system network-instance mgmt type ip-vrf
set / system network-instance mgmt admin-state enable
set / system network-instance mgmt description "Interface de management de %%%%"
set / system network-instance mgmt interface mgmt0.0 {}
set / system network-instance mgmt protocols linux export-routes true
set / system network-instance mgmt protocols linux export-neighbors true
##########  Device access daemons & VRF  ###########
set / system ssh-server network-instance mgmt admin-state enable
set / system ssh-server network-instance mgmt source-address [ 1.1.1.1 1.1.1.2 ]
set / system ssh-server network-instance tools admin-state enable
set / system ssh-server network-instance tools source-address [ 2.1.1.1 2.1.1.2 ]
######### regles RABC / autorization ################
set / system aaa authorization role operator-deployer rule / action write
set / system aaa authorization role operator-auditor rule / action read
set / system aaa authorization role security-auditor rule / action read
##########  Comptes M2M  ############################
set / system aaa authentication user operator-auditor ssh-key [ ssh-rsa  ] password reader
set / system aaa authentication user operator-auditor role [ operator-auditor ]
set / system aaa authentication user operator-deployer ssh-key [ ssh-rsa  ]
set / system aaa authentication user operator-deployer role [ operator-deployer ]
set / system aaa authentication user security-auditor ssh-key [ ssh-rsa  ] password reader
set / system aaa authentication user security-auditor role [ security-auditor ]
############## Services d'authentification ###########
set / system aaa authentication authentication-method [ local tacacs-all ]
set / system aaa authentication idle-timeout 60
############ Banniere / motd   #######################
set / system banner login-banner "Login banner message"
set / system banner motd-banner "Penetrer dans un systeme de traitement automatise....."
############   NTP ###################################
set / system ntp admin-state enable
set / system ntp network-instance mgmt
set / system ntp server 10.171.50.52
set / system ntp server 10.104.78.61
set / system clock timezone Europe/Paris
########## Legacy SNMP ##############################
set / system snmp community phare-reader permission r
set / system snmp community phare-reader version v2c
set / system snmp network-instance mgmt admin-state enable
set / system snmp network-instance mgmt source-address [ 1.1.1.1 1.1.1.2 ]




